{
  "extends": [
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {}
  },
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "array-element-newline": ["error", "never"],
    "array-bracket-newline": ["error", {
      "multiline": true
    }],
    "no-empty": 2
  }
}